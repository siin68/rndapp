    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "next": "^14.2.0",
    "next-auth": "^4.24.5",
    "next-intl": "^3.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
